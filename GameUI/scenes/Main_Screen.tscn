script/source = "extends GridContainer


const WhatGame = preload(\"res://GameUI/scenes/Component/whatGame.gd\")
#@onready var score: Label = $MarginContainer/Score
var unsortedMaxPlayCount:Array
var gameAndScore:Dictionary

var sortedPlayCounts:Array

func _ready() -> void:
	call_deferred(\"initialize\")
	
func initialize():
	for child in get_children():
		var game = WhatGame.new(child.name)
		#this means it has no game data so we continue (Not Terminate) the function
		if not ProfileDataGlobals.saveData.has(game.gameName):
			continue
		var playCount:Array = ProfileDataGlobals.saveData[game.gameName].tries
		gameAndScore[child.name] = playCount.max()
		unsortedMaxPlayCount.append(playCount.max())
		#score.text = str(scores.max())
		pass

	#print(\"okay\")
	print(gameAndScore)
	#print(unsortedMaxPlayCount)
	sortedPlayCounts = merge_sort(unsortedMaxPlayCount)
	#print(merge_sort(unsortedMaxPlayCount))
	var sortedPlayCount = sortedPlayCounts.size() - 1
	var topPlayedCount:int = 0
	while sortedPlayCount >= 0:
		#print(sortedPlayCounts[sortedPlayCount])
		for key in gameAndScore:
			#print(key)
			if gameAndScore[key] == sortedPlayCounts[sortedPlayCount]: 
				#print(\"Found key:\", key, \"for value:\", sortedPlayCount)
				for child in get_children():
					if not child.name == key:
						continue
					move_child(child, topPlayedCount) 
					#print(child.name, \" = \", key)
				break
		sortedPlayCount -= 1
		topPlayedCount += 1
	pass

#func _ready() -> void:
	##var child = get_child(0)  
	##move_child(child, 2) 
	#pass

# Call this function with your array to sort it
func merge_sort(arr: Array) -> Array:
	if arr.size() <= 1:
		return arr
	
	var mid = arr.size() / 2
	var left = merge_sort(arr.slice(0, mid))
	var right = merge_sort(arr.slice(mid, arr.size()))

	return merge(left, right)

func merge(left: Array, right: Array) -> Array:
	var result = []
	var i = 0
	var j = 0

	while i < left.size() and j < right.size():
		if left[i] <= right[j]:
			result.append(left[i])
			i += 1
		else:
			result.append(right[j])
			j += 1

	while i < left.size():
		result.append(left[i])
		i += 1

	while j < right.size():
		result.append(right[j])
		j += 1

	return result
"

