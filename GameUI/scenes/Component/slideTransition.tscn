[sub_resource type="GDScript" id="GDScript_2o8gl"]
script/source = "extends Control
@onready var animation_player: AnimationPlayer = $AnimationPlayer

func _ready() -> void:
	hide()
	globalGameData.connect(\"transition\",transIn)
	if globalGameData.isTransitionPlayed:
		show()
		transOut()
func transIn(pathToNextScene:String):
	show()
	animation_player.play(\"slideTransition\")
	await animation_player.animation_finished
	globalGameData.isTransitionPlayed = true
	#get_tree().change_scene_to_file(\"res://GameUI/scenes/Main_Screen.tscn\")
	get_tree().change_scene_to_file(pathToNextScene)

func transOut():
	globalGameData.isTransitionPlayed = false
	animation_player.play(\"slideTransitionOut\")
	await animation_player.animation_finished
	hide()
	pass
"

