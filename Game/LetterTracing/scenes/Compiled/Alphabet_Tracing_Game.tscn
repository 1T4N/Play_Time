[sub_resource type="GDScript" id="GDScript_6o2um"]
script/source = "extends Control
var childrens:Array
var currentLetterCardPointer:int = -1

func _ready() -> void:
	childrens = get_children()
	print(childrens)
	for child in childrens:
		child.hide()
	unPauseNextCard()

func unPauseNextCard():
	#print(currentLetterCardPointer, childrens[currentLetterCardPointer])
	if abs(currentLetterCardPointer) > childrens.size():
		return
	
	childrens[currentLetterCardPointer].show()
	print(\"First card pointer: \",currentLetterCardPointer, \" Card: \", childrens[currentLetterCardPointer])
	currentLetterCardPointer -= 1
	childrens[currentLetterCardPointer].show()
	print(\"2nd card pointer: \",currentLetterCardPointer, \" Card: \", childrens[currentLetterCardPointer])
	currentLetterCardPointer -= 1
	
"

