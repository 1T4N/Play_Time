script/source = "extends Button
var menuComponent

func _ready() -> void:
	menuComponent = get_node_or_null(\"../../../../Menu\")

func _on_pressed() -> void:
	if menuComponent == null:
		return

	menuComponent.show()
	get_tree().paused = true
"

